function ff=f(xx,yy,k,tt)

global alpha

%       ff(xx,yy,t) = {f1,f2}'
    
if k==1     % Primera componente del vector ff (f1)          
%    ff=alpha*tt;
%    ff=1;
    ff=lambda + alpha*(-1 + 2*xx)*(-1 + yy)*yy - 2*lambda*yy**2 + lambda*xx*(-2 + 4*y) + mu*(1 - 2*xx**2 + 2*y + 4*xx*y - 4*y**2);
else        % Segunda componente del vector ff (f2)   
%    ff=0;
    ff=alpha*(-1 + xx)*xx*(-1 + 2* yy);
end
       
return
end